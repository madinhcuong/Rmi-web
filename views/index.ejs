<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('component/head') %>
</head>

<body>
    <!-- ##### Preloader ##### -->
    <div id="preloader">
        <i class="circle-preloader"></i>
    </div>

    <!-- ##### Header Area Start ##### -->
    <header class="header-area">

        <!-- Top Header Area -->
        <div class="top-header">
            <%- include('component/header') %>
        </div>

        <!-- Navbar Area -->
        <div class="academy-main-menu">
            <%- include('component/menu') %>
        </div>
    </header>
    <!-- ##### Header Area End ##### -->

    <!-- ##### Hero Area Start ##### -->
    <section class="hero-area">
        <div class="hero-slides owl-carousel">
            <%- include('page/home/carousel') %>
        </div>
    </section>
    <!-- ##### Hero Area End ##### -->

    <!-- ##### Top Popular Courses Area Start ##### -->
    <div class="top-popular-courses-area section-padding-100-70">
        <%- include('page/home/course') %>
    </div>
    <!-- ##### Top Popular Courses Area End ##### -->
    <!-- ##### Partner Area Start ##### -->
    <div class="partner-area section-padding-0-100" id="respon-chungchi">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="section-heading text-center mx-auto wow fadeInUp" data-wow-delay="300ms">
                        <h3> CHỨNG CHỈ NGHIỆP VỤ</h3>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-12">
                    <div id="chungchi"
                        class="partners-logo d-flex align-items-center justify-content-between flex-wrap">
                        <a href="#" hidden=""><img src="/img/riam/hoptac/cc1.jpg" alt=""></a>
                        <a href="#"><img src="/img/riam/hoptac/cc1.jpg" alt=""></a>
                        <a href="#"><img src="/img/riam/hoptac/cc2.jpg" alt=""></a>
                        <a href="#"><img src="/img/riam/hoptac/cc3.jpg" alt=""></a>
                        <a href="#" hidden><img src="/img/riam/hoptac/cc1.jpg" alt=""></a>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- ##### Partner Area End ##### -->
    <!-- ##### Course Area Start ##### -->
    <div class="academy-courses-area section-padding-100-0">
        <div class="container" style="margin-top: -10%;">
            <div class="row">
                <!-- Single Course Area -->
                <%- include('page/home/training') %>
            </div>
        </div>
    </div>
    <!-- ##### Course Area End ##### -->
    <section class="contact-area">
        <%- include('page/home/contact') %>
    </section>

    <!-- ##### Footer Area Start ##### -->
    <footer class="footer-area">
        <%- include('component/footer') %>
    </footer>
    <!-- ##### Footer Area Start ##### -->

    <!-- ##### All Javascript Script ##### -->
    <%- include('component/script') %>
</body>

</html>